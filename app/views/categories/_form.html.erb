<%= form_for @category do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <%= form.label :parent_id %>
  <%= form.select :parent_id, options_for_select(Category.all.map{ |c| [c.title, c.id] }) %>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <%= form.submit "Submit" %>
<% end %>